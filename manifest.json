{
  "manifest_version": 3,
  "name": "PizzaDAO OS",
  "version": "1.0",
  "description": "A PizzaDAO themed desktop interface for your New Tab page.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; frame-src https://www.youtube.com; media-src 'self'"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": []
}